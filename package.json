{
  "name": "slatch",
  "displayName": "slatch",
  "description": "",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/FAReTek1/slatch.git"
  },
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "slatch.sb3",
        "displayName": "sb3 project runner",
        "selector": [
          {
            "filenamePattern": "*.sb3"
          }
        ],
        "priority": "default"
      }
    ],

    "viewsContainers": {
      "activitybar": [
        {
          "title": "Slatch",
          "id": "slatch_viewContainer",
          "icon": "media/wobo.svg"
        }
      ]
    },
    "views": {
      "slatch_viewContainer": [
        {
          "id": "slatch.commandView",
          "name": "Commands"
        }
      ]
    },
    "commands": [
      {
        "command": "slatch.Tree.select",
        "title": "Slatch: select a tree node (INTERNAL)"
      },
      {
        "command": "slatch.sb3.open",
        "title": "open an sb3 file"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "slatch.Tree.select",
          "when": "false"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.103.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "jsdom": "^26.1.0"
  }
}
